{
  "name": "WheelOfJeopardyFirebase",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "build": "npm run clean && npm run build:funcs && npm run deps:copy && npm run deps:install && npm run keys:copy",
    "clean": "rimraf 'dist/functions'",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "build:funcs": "babel src --out-dir dist/functions",
    "deps:copy": "cpx '*{package.json,yarn.lock}' 'dist/functions'",
    "deps:install": "cd 'dist/functions' && yarn",
    "keys:copy": "cpx 'src/graphql/utils/jwt.keys.json' 'dist/functions/'"
  },
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "cpx": "^1.5.0",
    "eslint": "^6.1.0",
    "firebase-tools": "^7.2.2",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@firebase/app-types": "^0.4.3",
    "@google-cloud/firestore": "^2.2.6",
    "apollo-server-core": "^2.8.1",
    "apollo-server-express": "^2.8.1",
    "apollo-utilities": "^1.3.2",
    "avatar-initials": "^4.1.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-graphql": "^0.9.0",
    "faker": "^4.1.0",
    "firebase": "^6.3.5",
    "firebase-admin": "^8.3.0",
    "firebase-functions": "^3.2.0",
    "googleapis": "^42.0.0",
    "graphql": "^14.4.2",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-server-express": "^1.4.1",
    "graphql-tools": "^4.0.5",
    "hash.js": "^1.1.7",
    "ip6addr": "^0.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "merge-graphql-schemas": "^1.7.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "moment": "^2.24.0",
    "no-avatar": "^1.0.1",
    "secure-compare": "^3.0.1",
    "super-graphiql-express": "^0.0.2",
    "uuid": "^3.3.2"
  }
}
